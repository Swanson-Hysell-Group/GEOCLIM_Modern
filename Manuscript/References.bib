@article{Stolper2016a,
	Abstract = {The history of atmospheric O2 partial pressures (Po2) is inextricably linked to the coevolution of life and Earth{\textquoteright}s biogeochemical cycles. Reconstructions of past Po2 rely on models and proxies but often markedly disagree. We present a record of Po2 reconstructed using O2/N2 ratios from ancient air trapped in ice. This record indicates that Po2 declined by 7 per mil (0.7\%) over the past 800,000 years, requiring that O2 sinks were ~2\% larger than sources. This decline is consistent with changes in burial and weathering fluxes of organic carbon and pyrite driven by either Neogene cooling or increasing Pleistocene erosion rates. The 800,000-year record of steady average carbon dioxide partial pressures (Pco2) but declining Po2 provides distinctive evidence that a silicate weathering feedback stabilizes Pco2 on million-year time scales.},
	Author = {Stolper, D. A. and Bender, M. L. and Dreyfus, G. B. and Yan, Y. and Higgins, J. A.},
	Date-Added = {2017-04-08 00:57:47 +0000},
