	Journal = {Geology},
	Number = {9},
	Pages = {817-820},
	Title = {{U}-{P}b zircon date from the {N}eoproterozoic {G}haub {F}ormation, {N}amibia: Constraints on {M}arinoan glaciation},
	Url = {http://geology.gsapubs.org/content/32/9/817.abstract},
	Volume = {32},
