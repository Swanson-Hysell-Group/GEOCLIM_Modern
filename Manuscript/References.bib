	Issn = {1941-8264},
	Journal = {Lithosphere},
	Number = {3},
	Pages = {275-295},
	Publisher = {Geological Society of America},
	Title = {{K}ikiktat volcanics of {A}rctic {A}laska-Melting of harzburgitic mantle associated with the {F}ranklin large igneous province},
