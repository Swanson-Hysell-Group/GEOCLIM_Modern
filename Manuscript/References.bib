@article{Zhou2004a,
	Abstract = {The most complete Neoproterozoic successions in south China contain three diamictite intervals in the Changan, Tiesiao, and Nantuo Formations. The youngest and most widespread Nantuo glacial deposit overlies the Datangpo Formation and underlies the fossil- rich Doushantuo Formation. Previous authors have correlated the Nantuo diamictite to either Sturtian or Marinoan glacial deposits elsewhere. Here we report sedimentary and δ13C chemostratigraphic data of the Doushantuo cap dolostone, which overlies the Nantuo Formation. Facies-dependent variation in δ13C is interpreted as evidence for spatial heterogeneity in δ13C and/or temporal diachroneity in the initiation of cap carbonate sedimentation. Sedimentary and chemostratigraphic data are indicative of a Marinoan age for the Nantuo glaciation. This inference is supported by a new U-Pb zircon age of 663 $\pm$ 4 Ma from a tuffaceous bed in the Datangpo Formation. The new date and other isotopic ages from south China constrain the age of the Changan and Tiesiao glaciation(s) as between 761 $\pm$ 8 Ma and 663 $\pm$ 4 Ma, and the Nantuo glaciation as between 663 $\pm$ 4 Ma and 599 $\pm$ 4 Ma.},
	Author = {Zhou, Chuanming and Tucker, Robert and Xiao, Shuhai and Peng, Zhanxiong and Yuan, Xunlai and Chen, Zhe},
	Date-Added = {2016-12-12 03:38:05 +0000},
	Date-Modified = {2017-03-17 20:14:49 +0000},
	Doi = {10.1130/G20286.1},
	Eprint = {http://geology.gsapubs.org/content/32/5/437.full.pdf+html},
	Journal = {Geology},
	Number = {5},
	Pages = {437-440},
	Title = {New constraints on the ages of {N}eoproterozoic glaciations in south {C}hina},
	Url = {http://geology.gsapubs.org/content/32/5/437.abstract},
	Volume = {32},
	Year = {2004},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://geology.gsapubs.org/content/32/5/437.abstract},
