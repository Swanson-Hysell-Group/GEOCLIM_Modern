	Issn = {0301-9268},
	Journal = {Precambrian Research},
	Keywords = {Neoproterozoic, Sunspot cycle, Siliciclastic biolaminites, Annual lamination, South China},
	Pages = {75 - 91},
	Title = {Sunspot cycles recorded in siliciclastic biolaminites at the dawn of the {N}eoproterozoic {S}turtian glaciation in {S}outh {C}hina},
