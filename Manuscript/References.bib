	Doi = {10.1073/pnas.1607712113},
	Eprint = {http://www.pnas.org/content/113/52/14904.full.pdf},
	Journal = {Proceedings of the National Academy of Sciences},
	Number = {52},
	Pages = {14904-14909},
	Title = {Episode of intense chemical weathering during the termination of the 635 {M}a {M}arinoan glaciation},
	Url = {http://www.pnas.org/content/113/52/14904.abstract},
	Volume = {113},
