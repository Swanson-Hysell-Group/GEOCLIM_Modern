	Journal = {Journal of Climate},
	Number = {16},
	Pages = {4402-4418},
	Title = {Atmospheric and Surface Contributions to Planetary Albedo},
	Url = {http://dx.doi.org/10.1175/2011JCLI3946.1},
	Volume = {24},
	Year = {2011},
