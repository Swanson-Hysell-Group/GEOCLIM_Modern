	Eprint = {http://geology.gsapubs.org/content/early/2017/03/27/G38931.1.full.pdf+html},
	Journal = {Geology},
	Title = {The rise and fall of stromatolites in shallow marine environments},
	Url = {http://geology.gsapubs.org/content/early/2017/03/27/G38931.1.abstract},
	Year = {2017},
	Bdsk-File-1 = {},
