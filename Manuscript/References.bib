	M3 = {Article},
	Month = {02},
	Number = {2},
	Pages = {135-139},
	Publisher = {Nature Publishing Group},
	Title = {A key role for green rust in the {P}recambrian oceans and the genesis of iron formations},
	Ty = {JOUR},
