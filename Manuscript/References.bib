	Issn = {0036-8075},
	Journal = {Science},
	Number = {6357},
	Pages = {1271-1274},
	Publisher = {American Association for the Advancement of Science},
	Title = {Titanium isotopic evidence for felsic crust and plate tectonics 3.5 billion years ago},
