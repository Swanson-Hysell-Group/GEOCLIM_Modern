	Journal = {Geology},
	Number = {6},
	Pages = {475-478},
	Title = {Magmatic activity and plate motion during the latent stage of {M}idcontinent {R}ift development},
	Url = {http://geology.gsapubs.org/content/42/6/475.abstract},
	Volume = {42},
