	Eprint = {http://geology.gsapubs.org/content/22/4/295.full.pdf+html},
	Journal = {Geology},
	Number = {4},
	Pages = {295-298},
	Title = {Bathymetric and isotopic evidence for a short-lived {L}ate {O}rdovician glaciation in a greenhouse period},
	Url = {http://geology.gsapubs.org/content/22/4/295.abstract},
	Volume = {22},
