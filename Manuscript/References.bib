	Journal = {Journal of the Geological Society},
	Number = {1},
	Pages = {183-191},
	Title = {Constraints on $^{87}${S}r/$^{86}${S}r of {L}ate {E}diacaran seawater: insight from {S}iberian high-{S}r limestones},
