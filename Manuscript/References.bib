	Journal = {Proceedings of the National Academy of Sciences},
	Number = {19},
	Pages = {8543-8548},
	Title = {Calcium isotope constraints on the end-{P}ermian mass extinction},
	Url = {http://www.pnas.org/content/107/19/8543.abstract},
