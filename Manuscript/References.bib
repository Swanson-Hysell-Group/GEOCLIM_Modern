	Journal = {Paleoceanography},
	Keywords = {Neoproterozoic, snowball Earth, paleoclimate},
	Number = {4},
	Pages = {1-8},
	Title = {{N}eoproterozoic ``snowball {}Earth'': Dynamic sea ice over a quiescent ocean},
