	Journal = {Journal of Earth Science},
	Number = {2},
	Pages = {225-232},
	Title = {The Pre-{S}turtian negative $\delta^{13}${C} excursion of the {D}ajiangbian formation deposited on the western margin of {C}athaysia {B}lock in {S}outh {C}hina},
