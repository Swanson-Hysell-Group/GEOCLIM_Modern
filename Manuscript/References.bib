	Issn = {1437-3262},
	Journal = {International Journal of Earth Sciences},
	Number = {3},
	Pages = {521-539},
	Title = {Sedimentary successions and the onset of the {N}eoproterozoic {J}iangnan sub-basin in the {N}anhua rift, {S}outh {C}hina},
	Url = {http://dx.doi.org/10.1007/s00531-014-1107-5},
