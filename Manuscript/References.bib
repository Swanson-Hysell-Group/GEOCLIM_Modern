	Journal = {Paleoceanography},
	Keywords = {Maritime Continent, Ice Ages, Walker Circulation, El Nino, Pliocene, past pCO2},
	Number = {3},
	Pages = {196-225},
	Title = {Growth of the {M}aritime {C}ontinent and its possible contribution to recurring Ice Ages},
