	Journal = {Geochemistry, Geophysics, Geosystems},
	Keywords = {data set, global map, lithology},
	Number = {12},
	Title = {The new global lithological map database {GLiM}: A representation of rock properties at the {E}arth surface},
	Url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2012GC004370},
