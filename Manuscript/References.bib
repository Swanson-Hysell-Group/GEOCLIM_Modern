	Journal = {Geological Society of America Bulletin},
	Number = {11-12},
	Pages = {1551-1568},
	Title = {Calibration of a conodont apatite-based {O}rdovician $^{87}${S}r/$^{86}${S}r curve to biostratigraphy and geochronology: Implications for stratigraphic resolution},
