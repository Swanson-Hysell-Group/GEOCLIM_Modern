	Eprint = {http://geology.gsapubs.org/content/34/12/1073.full.pdf+html},
	Journal = {Geology},
	Number = {12},
	Pages = {1073-1076},
	Title = {Origin of hummocky and swaley cross-stratification- The controlling influence of unidirectional current strength and aggradation rate},
	Url = {http://geology.gsapubs.org/content/34/12/1073.abstract},
	Volume = {34},
	Year = {2006},
