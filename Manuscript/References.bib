	Doi = {10.1130/G38602.1},
	Eprint = {http://geology.geoscienceworld.org/content/early/2017/01/09/G38602.1.full.pdf},
	Issn = {0091-7613},
	Journal = {Geology},
	Publisher = {Geological Society of America},
	Title = {Bacterial sulfur disproportionation constrains timing of {N}eoproterozoic oxygenation},
	Url = {http://geology.geoscienceworld.org/content/early/2017/01/09/G38602.1},
	Year = {2017},
	Bdsk-File-1 = {},
