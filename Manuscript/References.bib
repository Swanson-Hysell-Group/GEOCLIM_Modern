@article{Schrag2013a,
	Abstract = {We present a framework for interpreting the carbon isotopic composition of sedimentary rocks, which in turn requires a fundamental reinterpretation of the carbon cycle and redox budgets over Earth{\textquoteright}s history. We propose that authigenic carbonate, produced in sediment pore fluids during early diagenesis, has played a major role in the carbon cycle in the past. This sink constitutes a minor component of the carbon isotope mass balance under the modern, high levels of atmospheric oxygen but was much larger in times of low atmospheric O2 or widespread marine anoxia. Waxing and waning of a global authigenic carbonate sink helps to explain extreme carbon isotope variations in the Proterozoic, Paleozoic, and Triassic.},
	Author = {Schrag, Daniel P. and Higgins, John. A. and Macdonald, Francis A. and Johnston, David T.},
